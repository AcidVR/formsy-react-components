{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "extends": "plugin:react/recommended",
    "rules": {
        "strict": [2, "global"],
        "quotes": [2, "single"],
        "no-dupe-keys": 2,
        "no-unused-vars": 2,
        "camelcase": 2,
        "no-underscore-dangle": 0
    }
}
